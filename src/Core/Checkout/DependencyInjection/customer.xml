<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Laser\Core\Checkout\Customer\CustomerDefinition">
            <tag name="laser.entity.definition"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Aggregate\CustomerGroupTranslation\CustomerGroupTranslationDefinition">
            <tag name="laser.entity.definition"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Aggregate\CustomerAddress\CustomerAddressDefinition">
            <tag name="laser.entity.definition"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Aggregate\CustomerRecovery\CustomerRecoveryDefinition">
            <tag name="laser.entity.definition"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Aggregate\CustomerGroup\CustomerGroupDefinition">
            <tag name="laser.entity.definition"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Aggregate\CustomerGroupRegistrationSalesChannel\CustomerGroupRegistrationSalesChannelDefinition">
            <tag name="laser.entity.definition"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Aggregate\CustomerTag\CustomerTagDefinition">
            <tag name="laser.entity.definition"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\AccountService">
            <argument type="service" id="customer.repository"/>
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="Laser\Core\Checkout\Customer\Password\LegacyPasswordVerifier"/>
            <argument type="service" id="Laser\Core\Checkout\Customer\SalesChannel\SwitchDefaultAddressRoute"/>
            <argument type="service" id="Laser\Core\System\SalesChannel\Context\CartRestorer"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Validation\AddressValidationFactory">
            <argument type="service" id="Laser\Core\System\SystemConfig\SystemConfigService"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Validation\CustomerProfileValidationFactory">
            <argument type="service" id="Laser\Core\System\Salutation\SalutationDefinition"/>
            <argument type="service" id="Laser\Core\System\SystemConfig\SystemConfigService"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Validation\CustomerValidationFactory">
            <argument type="service" id="Laser\Core\Checkout\Customer\Validation\CustomerProfileValidationFactory"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Validation\Constraint\CustomerEmailUniqueValidator">
            <argument type="service" id="Doctrine\DBAL\Connection"/>

            <tag name="validator.constraint_validator"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Validation\Constraint\CustomerPasswordMatchesValidator">
            <argument type="service" id="Laser\Core\Checkout\Customer\SalesChannel\AccountService"/>

            <tag name="validator.constraint_validator"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Validation\Constraint\CustomerVatIdentificationValidator">
            <argument type="service" id="Doctrine\DBAL\Connection"/>
            <tag name="validator.constraint_validator"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Validation\Constraint\CustomerZipCodeValidator">
            <argument type="service" id="country.repository"/>
            <tag name="validator.constraint_validator"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Password\LegacyEncoder\Md5">
            <tag name="laser.legacy_encoder"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Password\LegacyEncoder\Sha256">
            <tag name="laser.legacy_encoder"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Password\LegacyPasswordVerifier">
            <argument type="tagged" tag="laser.legacy_encoder" />
        </service>

        <service id="Laser\Core\Checkout\Customer\Subscriber\CustomerMetaFieldSubscriber">
            <argument type="service" id="Doctrine\DBAL\Connection"/>

            <tag name="kernel.event_subscriber"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Subscriber\CustomerRemoteAddressSubscriber">
            <argument type="service" id="customer.repository"/>
            <argument type="service" id="request_stack"/>

            <tag name="kernel.event_subscriber"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Subscriber\CustomerTokenSubscriber">
            <argument type="service" id="Laser\Core\System\SalesChannel\Context\SalesChannelContextPersister"/>
            <argument type="service" id="request_stack"/>
            <tag name="kernel.event_subscriber"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Subscriber\CustomerGroupSubscriber">
            <argument type="service" id="customer_group.repository"/>
            <argument type="service" id="seo_url.repository"/>
            <argument type="service" id="language.repository"/>
            <argument type="service" id="Laser\Core\Content\Seo\SeoUrlPersister"/>
            <argument type="service" id="slugify"/>
            <tag name="kernel.event_subscriber"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Subscriber\CustomerChangePasswordSubscriber">
            <argument type="service" id="Doctrine\DBAL\Connection"/>
            <tag name="kernel.event_subscriber"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Subscriber\CustomerFlowEventsSubscriber">
            <argument type="service" id="Symfony\Component\EventDispatcher\EventDispatcherInterface" />
            <argument type="service" id="Laser\Core\System\SalesChannel\Context\SalesChannelContextRestorer" />
            <argument type="service" id="Laser\Core\Checkout\Customer\DataAbstractionLayer\CustomerIndexer" />
            <tag name="kernel.event_subscriber"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\LoginRoute" public="true">
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="Laser\Core\Checkout\Customer\SalesChannel\AccountService"/>
            <argument type="service" id="customer.repository"/>
            <argument type="service" id="Laser\Core\System\SalesChannel\Context\CartRestorer"/>
            <argument type="service" id="Symfony\Component\HttpFoundation\RequestStack"/>
            <argument type="service" id="Laser\Core\Framework\RateLimiter\RateLimiter"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\LogoutRoute" public="true">
            <argument type="service" id="Laser\Core\System\SalesChannel\Context\SalesChannelContextPersister"/>
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="Laser\Core\System\SystemConfig\SystemConfigService"/>
            <argument type="service" id="Laser\Core\Checkout\Cart\SalesChannel\CartService"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\SendPasswordRecoveryMailRoute" public="true">
            <argument type="service" id="customer.repository"/>
            <argument type="service" id="customer_recovery.repository"/>
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="Laser\Core\Framework\Validation\DataValidator"/>
            <argument type="service" id="Laser\Core\System\SystemConfig\SystemConfigService"/>
            <argument type="service" id="Symfony\Component\HttpFoundation\RequestStack"/>
            <argument type="service" id="laser.rate_limiter"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\ResetPasswordRoute" public="true">
            <argument type="service" id="customer.repository"/>
            <argument type="service" id="customer_recovery.repository"/>
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="Laser\Core\Framework\Validation\DataValidator"/>
            <argument type="service" id="Laser\Core\System\SystemConfig\SystemConfigService"/>
            <argument type="service" id="Symfony\Component\HttpFoundation\RequestStack"/>
            <argument type="service" id="laser.rate_limiter"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\CustomerRecoveryIsExpiredRoute" public="true">
            <argument type="service" id="customer_recovery.repository"/>
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="Laser\Core\Framework\Validation\DataValidator"/>
            <argument type="service" id="Laser\Core\System\SystemConfig\SystemConfigService"/>
            <argument type="service" id="Symfony\Component\HttpFoundation\RequestStack"/>
            <argument type="service" id="laser.rate_limiter"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\ChangePaymentMethodRoute" public="true">
            <argument type="service" id="customer.repository"/>
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="payment_method.repository"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\ChangeCustomerProfileRoute" public="true">
            <argument type="service" id="customer.repository"/>
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="Laser\Core\Framework\Validation\DataValidator"/>
            <argument type="service" id="Laser\Core\Checkout\Customer\Validation\CustomerProfileValidationFactory"/>
            <argument type="service" id="Laser\Core\System\SalesChannel\StoreApiCustomFieldMapper"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\ChangePasswordRoute" public="true">
            <argument type="service" id="customer.repository"/>
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="Laser\Core\System\SystemConfig\SystemConfigService"/>
            <argument type="service" id="Laser\Core\Framework\Validation\DataValidator"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\ChangeEmailRoute" public="true">
            <argument type="service" id="customer.repository"/>
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="Laser\Core\Framework\Validation\DataValidator"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\ChangeLanguageRoute" public="true">
            <argument type="service" id="customer.repository"/>
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="Laser\Core\Framework\Validation\DataValidator"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\CustomerRoute" public="true">
            <argument type="service" id="customer.repository"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\DeleteCustomerRoute" public="true">
            <argument type="service" id="customer.repository"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\RegisterRoute" public="true">
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="Laser\Core\System\NumberRange\ValueGenerator\NumberRangeValueGeneratorInterface"/>
            <argument type="service" id="Laser\Core\Framework\Validation\DataValidator"/>
            <argument type="service" id="Laser\Core\Checkout\Customer\Validation\CustomerValidationFactory"/>
            <argument type="service" id="Laser\Core\Checkout\Customer\Validation\AddressValidationFactory"/>
            <argument type="service" id="Laser\Core\System\SystemConfig\SystemConfigService"/>
            <argument type="service" id="customer.repository"/>
            <argument type="service" id="Laser\Core\System\SalesChannel\Context\SalesChannelContextPersister"/>
            <argument type="service" id="sales_channel.country.repository"/>
            <argument type="service" id="Doctrine\DBAL\Connection"/>
            <argument type="service" id="Laser\Core\System\SalesChannel\Context\SalesChannelContextService"/>
            <argument type="service" id="Laser\Core\System\SalesChannel\StoreApiCustomFieldMapper"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\RegisterConfirmRoute" public="true">
            <argument type="service" id="customer.repository"/>
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="Laser\Core\Framework\Validation\DataValidator"/>
            <argument type="service" id="Laser\Core\System\SalesChannel\Context\SalesChannelContextPersister"/>
            <argument type="service" id="Laser\Core\System\SalesChannel\Context\SalesChannelContextService"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\ListAddressRoute" public="true">
            <argument type="service" id="customer_address.repository"/>
            <argument type="service" id="event_dispatcher"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\UpsertAddressRoute" public="true">
            <argument type="service" id="customer_address.repository"/>
            <argument type="service" id="Laser\Core\Framework\Validation\DataValidator"/>
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="Laser\Core\Checkout\Customer\Validation\AddressValidationFactory"/>
            <argument type="service" id="Laser\Core\System\SystemConfig\SystemConfigService"/>
            <argument type="service" id="Laser\Core\System\SalesChannel\StoreApiCustomFieldMapper"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\DeleteAddressRoute" public="true">
            <argument type="service" id="customer_address.repository"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\SwitchDefaultAddressRoute" public="true">
            <argument type="service" id="customer_address.repository"/>
            <argument type="service" id="customer.repository"/>
            <argument type="service" id="Symfony\Component\EventDispatcher\EventDispatcherInterface" />
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\CustomerGroupRegistrationSettingsRoute" public="true">
            <argument type="service" id="customer_group.repository"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\DataAbstractionLayer\CustomerIndexer">
            <argument type="service" id="Laser\Core\Framework\DataAbstractionLayer\Dbal\Common\IteratorFactory"/>
            <argument type="service" id="customer.repository"/>
            <argument type="service" id="Laser\Core\Framework\DataAbstractionLayer\Indexing\ManyToManyIdFieldUpdater"/>
            <argument type="service" id="Laser\Core\Content\Newsletter\DataAbstractionLayer\Indexing\CustomerNewsletterSalesChannelsUpdater"/>
            <argument type="service" id="event_dispatcher"/>
            <tag name="laser.entity_indexer" priority="100"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Api\CustomerGroupRegistrationActionController" public="true">
            <argument type="service" id="customer.repository"/>
            <argument type="service" id="customer_group.repository" />
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="Laser\Core\System\SalesChannel\Context\SalesChannelContextRestorer" />
        </service>

        <service id="Laser\Core\Checkout\Customer\Aggregate\CustomerWishlist\CustomerWishlistDefinition">
            <tag name="laser.entity.definition"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\Aggregate\CustomerWishlistProduct\CustomerWishlistProductDefinition">
            <tag name="laser.entity.definition"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\LoadWishlistRoute" public="true">
            <argument type="service" id="customer_wishlist.repository"/>
            <argument type="service" id="sales_channel.product.repository"/>
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="Laser\Core\System\SystemConfig\SystemConfigService"/>
            <argument type="service" id="Laser\Core\Content\Product\SalesChannel\ProductCloseoutFilterFactory"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\AddWishlistProductRoute" public="true">
            <argument type="service" id="customer_wishlist.repository"/>
            <argument type="service" id="sales_channel.product.repository"/>
            <argument type="service" id="Laser\Core\System\SystemConfig\SystemConfigService"/>
            <argument type="service" id="event_dispatcher"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\RemoveWishlistProductRoute" public="true">
            <argument type="service" id="customer_wishlist.repository"/>
            <argument type="service" id="customer_wishlist_product.repository"/>
            <argument type="service" id="Laser\Core\System\SystemConfig\SystemConfigService"/>
            <argument type="service" id="event_dispatcher"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\DataAbstractionLayer\CustomerWishlistProductExceptionHandler">
            <tag name="laser.dal.exception_handler"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\MergeWishlistProductRoute" public="true">
            <argument type="service" id="customer_wishlist.repository"/>
            <argument type="service" id="sales_channel.product.repository"/>
            <argument type="service" id="Laser\Core\System\SystemConfig\SystemConfigService"/>
            <argument type="service" id="event_dispatcher"/>
            <argument type="service" id="Doctrine\DBAL\Connection"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\CustomerValueResolver">
            <tag name="controller.argument_value_resolver" priority="1002"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\AccountNewsletterRecipientRoute" public="true">
            <argument type="service" id="sales_channel.newsletter_recipient.repository" />
        </service>

        <service id="Laser\Core\Checkout\Customer\DeleteUnusedGuestCustomerService">
            <argument type="service" id="customer.repository" />
            <argument type="service" id="Laser\Core\System\SystemConfig\SystemConfigService" />
        </service>

        <service id="Laser\Core\Checkout\Customer\Command\DeleteUnusedGuestCustomersCommand">
            <argument type="service" id="Laser\Core\Checkout\Customer\DeleteUnusedGuestCustomerService" />
            <tag name="console.command" />
        </service>

        <service id="Laser\Core\Checkout\Customer\DeleteUnusedGuestCustomerHandler">
            <argument type="service" id="scheduled_task.repository"/>
            <argument type="service" id="Laser\Core\Checkout\Customer\DeleteUnusedGuestCustomerService" />

            <tag name="messenger.message_handler" />
        </service>

        <service id="Laser\Core\Checkout\Customer\Subscriber\CustomerBeforeDeleteSubscriber">
            <argument type="service" id="customer.repository"/>
            <argument type="service" id="Laser\Core\System\SalesChannel\Context\SalesChannelContextService" />
            <argument type="service" id="event_dispatcher"/>

            <tag name="kernel.event_subscriber"/>
        </service>

        <service id="Laser\Core\Checkout\Customer\SalesChannel\DownloadRoute" public="true">
            <argument type="service" id="order_line_item_download.repository"/>
            <argument type="service" id="Laser\Core\Content\Media\File\DownloadResponseGenerator"/>
        </service>
    </services>
</container>
